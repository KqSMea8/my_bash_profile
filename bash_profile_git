
#git
alias gs='git status'
alias gc='git commit'
alias gp='git push'
alias ga='git add .'

alias gd='git diff'
alias gdc='git diff --cached'
alias gdf='git diff --color | diff-so-fancy'

alias gba='git branch -av'
alias gbm='git checkout master'
alias gb='git checkout'

alias gsync='git pull --rebase;git push'
